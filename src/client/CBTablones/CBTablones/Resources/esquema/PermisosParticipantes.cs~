using System;
using SQLite;

namespace CBTablones
{
	// Tabla con la relación "tablón-contactos (participantes)"
	[Table("PERMISOSPARTICIPANTES")]
	public class PermisosParticipantes
	{
		#region Constructor
		public PermisosParticipantes()
		{			
		}
		#endregion

		#region Propiedades

		[PrimaryKey]
		public int TablonID{ get; set; }

		public int ContactID{ get; set; }

		[MaxLength(1)]
		public string Permiso{ get; set; }

		#endregion
	}
}
